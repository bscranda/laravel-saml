{
    "name": "bscranda/laravel-saml",
    "description": "SAML2 authentication for Laravel 6 applications using the OneLogin toolkit.",
    "authors": [
        {
            "name": "Brady Crandall",
            "email": "bscranda@svsu.edu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0",
        "onelogin/php-saml": "^3.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bscranda\\Saml\\SamlServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Bscranda\\Saml\\": "src/"
        }
    }
}
